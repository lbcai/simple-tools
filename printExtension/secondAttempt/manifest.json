{
  "name": "DymoInjecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replace LabArchives js file with a custom one to control print dimensions.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "*://inventory.labarchives.com/*",
    "tabs"
  ]
}